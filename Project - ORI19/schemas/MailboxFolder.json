{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MailboxFolder",
  "type": "object",
  "required": ["id", "mailboxId", "folderId", "folderPath", "createdUtc"],
  "properties": {
    "id": { "type": "string" },
    "mailboxId": { "type": "string" },
    "folderId": { "type": "string" },
    "folderPath": { "type": "string" },
    "wellKnownName": { "type": "string" },
    "itemCount": { "type": "integer" },
    "oldestItemDate": { "type": "string", "format": "date-time" },
    "newestItemDate": { "type": "string", "format": "date-time" },
    "createdUtc": { "type": "string", "format": "date-time" },
    "modifiedUtc": { "type": "string", "format": "date-time" }
  }
}
